type User {
  id: ID! @id
  name: String!
  lastName: String!
  bio: String!
  posts: [Post!]!
  likes: [Like!]!
  comments: [Comment!]!
}

tpye Post {
  location: String
  caption: String!
  user: User!
  files: [File!]!
  likes: [Like!]!
  comments: [Comment!]!
}

type Like {
  user: User!
  post: Post!
}

type Comment {
  text: String!
  user: User!
  post: Post!
}

type File {
  url: String!
  post: Post!
}